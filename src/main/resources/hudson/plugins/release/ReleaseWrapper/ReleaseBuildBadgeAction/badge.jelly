<!--
  Displays the form to choose the tag name.

  This belongs to a build view.
-->
<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:l="/lib/layout">
    
	<j:if test="${it.releaseVersion != null}">
      <l:icon tooltip="${it.releaseVersion}" alt="${it.releaseVersion}" class="icon-package icon-sm"/>
    </j:if>
    <j:if test="${it.releaseVersion == null}">
      <l:icon class="icon-package icon-sm"/>
    </j:if>
</j:jelly>
